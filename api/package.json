{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/main.ts",
    "start": "ts-node src/main.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate src/infrastructure/persistence/postgres/migrations/third --dataSource src/domain/market/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run --dataSource src/domain/market/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert",
    "seed": "ts-node src/domain/market/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.11.0",
    "axios": "^1.10.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.25",
    "zod": "^4.0.2"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.10",
    "prisma": "^6.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
